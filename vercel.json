{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist/hamza-resume/browser"
        }
      }
    ],
    
      "headers": [
        {
          "source": "/styles-*.css",
          "headers": [{ "key": "Content-Type", "value": "text/css" }]
        }
      ]
    ,
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  