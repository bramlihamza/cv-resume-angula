{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist/hamza-resume/browser"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ],

"rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ],

    "headers": [
    {
      "source": "*.css",
      "headers": [{ "key": "Content-Type", "value": "text/css" }]
    },
    {
      "source": "*.js",
      "headers": [{ "key": "Content-Type", "value": "text/js" }]
    }
  ]
  }
  